<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:Flotomachine.ViewModels"
             mc:Ignorable="d" d:DesignWidth="764" d:DesignHeight="420" 
             MaxWidth="764" MaxHeight="420"
             x:Class="Flotomachine.View.CameraPanelControl">
<UserControl.DataContext>
        <viewModels:CameraPanelControlViewModel/>
    </UserControl.DataContext>
	<Grid>
        <Image Source="{Binding MainImage}" Stretch="Uniform"></Image>
    </Grid>
</UserControl>
